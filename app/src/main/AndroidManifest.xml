<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.example.hrv.startpack">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD"/>
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" tools:ignore="ProtectedPermissions"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
            android:name="com.zskx.util.MyApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity
                android:name="com.zskx.activitys.LoginActivity"
                android:hardwareAccelerated="false"
                android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name="com.zskx.activitys.WebActivity"
                android:label="@string/title_activity_test_pem"
                android:screenOrientation="landscape"
                android:hardwareAccelerated="true"
                android:largeHeap="true"
                android:theme="@style/AppTheme.NoActionBar"/>
        <activity
                android:name="com.zskx.activitys.VideoActivity"
                android:label="@string/title_activity_test_video"
                android:screenOrientation="landscape"
                android:hardwareAccelerated="true"
                android:theme="@style/AppTheme.NoActionBar"/>
        <activity
                android:name="com.zskx.activitys.GuideActivity"
                android:label="@string/title_activity_test_guide"
                android:screenOrientation="landscape"
                android:hardwareAccelerated="false"
                android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.zskx.activitys.MainActivity"
            android:label="@string/title_activity_test_pem"
            android:screenOrientation="landscape"
            android:hardwareAccelerated="true"
            android:largeHeap="true"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.zskx.activitys.Report"
            android:label="@string/title_activity_test_pem"
            android:screenOrientation="landscape"
            android:hardwareAccelerated="true"
            android:largeHeap="true"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.zskx.activitys.Insert"
            android:label="@string/title_activity_test_pem"
            android:screenOrientation="landscape"
            android:hardwareAccelerated="true"
            android:largeHeap="true"
            android:theme="@style/AppTheme.NoActionBar"/>

        <uses-library android:name="com.android.future.usb.accessory"/>
    </application>

</manifest>